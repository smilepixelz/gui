SettingsAuraDisable.MouseButtonUp:Connect(function()
joe = false
end

SettingsFullHeal.MouseButtonUp:Connect(function()
local args = {
    [1] = Player.Character.Head.CFrame,
    [2] = math.huge,
    [3] = "rbxassetid://1202656211",
    [4] = 1.8,
    [5] = 0.2,
}

game:GetService("ReplicatedStorage").Heal:FireServer(unpack(args))
end

SettingsInfHealthRegen.MouseButtonUp:Connect(function()
local regentime = 4
repeat wait(regentime)
local args = {
    [1] = Player.Character.Head.CFrame,
    [2] = math.huge,
    [3] = "rbxassetid://1202656211",
    [4] = 1.8,
    [5] = 0.2,
}

game:GetService("ReplicatedStorage").Heal:FireServer(unpack(args))
until joe2 == false
end
